{
  "name": "cljs-chess",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "css:watch": "sass --watch src/css/base.css:public/css/base.min.css",
    "css:release": "sass src/css/base.css:public/css/base.min.css --style=compressed",
    "js:watch": "shadow-cljs watch app",
    "js:release": "shadow-cljs release app",
    "watch": "run-p -l *:watch",
    "release": "run-s *:release"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.13.0",
    "react-router-dom": "^6.8.0",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "shadow-cljs": "^2.20.20"
  }
}
